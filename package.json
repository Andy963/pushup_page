  "packageManager": "pnpm@8.9.0+sha256.8f5264ad1d100da11a6add6bb8a94c6f1e913f9e9261b2a551fabefad2ec0fec",
  "dependencies": {
    "@vercel/analytics": "^0.1.11",
    "@vitejs/plugin-react": "^5.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.15.0",
    "recharts": "^2.15.2",
